{# Below we include ("extend") the "base.html.twig" file #}
{% extends "base.html.twig" %}


{# Override the tile of the "base.html.twig" file #}
{% block title %}
    Movies Page
{% endblock %}

{# Override the body/content of the "base.html.twig" file #}
{% block body %}
    {% for movie in movies %}
        <li>{{ movie }}</li>
    {% endfor %}
{% endblock %}